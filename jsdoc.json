{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "experimentalDecorators": true,
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}